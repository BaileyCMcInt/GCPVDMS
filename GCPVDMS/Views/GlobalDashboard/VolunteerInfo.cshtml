@model ApplicationUser
@{
    Layout = "_Layout";

}


<partial name="_GlobalVolunteerTabs" />

<br/>

<center><h3 style="background-color: #009E00; color: white">@Model.FirstName @Model.LastName's Volunteer Information</h3></center>
<b>First Name:</b> @Model.FirstName<br />
<b>Last Name:</b> @Model.LastName <br />
<b>DOB:</b> @Model.Birthday.ToShortDateString() <br />
<b>County:</b> @Model.County <br/>
<b>Primary Method of Contact:</b> @Model.PreferredContact <br/>
<b>Secondary Method Contact:</b> @Model.SecondPreferredContact <br />
<b>Phone Number: </b>@Model.PhoneNumber<br/>
<b>Email:</b> @Model.Email<br/>

<br />
<center> <a asp-action="" class="btn btn-outline-dark">Print</a> <a asp-action="viewVolunteers" class="btn btn-outline-dark">Back to Volunteer List</a></center>