
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GCPVDMS</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @*DataTables CDN*@
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.css">
    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.js"></script>

    @RenderSection("Scripts", required: false)
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-md navbar-light box-shadow">
            @*<a class="navbar-brand gcplogo" href="/">
                <img src="/assets/images/LogoRound.png" alt="The Giving Closet Project" />
            </a>*@
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-end" id="navbarCollapse">
                <ul class="navbar-nav">

                    <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Index">Donate</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Index">Referral</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Index">Programs</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Event" asp-action="EventSignUp">Volunteer</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Index">About</a></li>
                    <li class="nav-item"><a class="nav-link" asp-controller="Home" asp-action="Index">Locations</a></li>

                    <partial name="_LoginPartial.cshtml" />
                </ul>

            </div>
        </nav>

       
    </header>
    <div class="container">
        <div class="col-md-3">
        </div>
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>



    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - The Giving Closet Project. All Rights Reserved.  <a asp-area="" asp-controller="Home" asp-action="Index">Contact Us</a>
            <a asp-area="" asp-controller="Home" asp-action="Index">Donate</a>
            <a asp-area="" asp-controller="Home" asp-action="Index">Referral</a>
            <a asp-area="" asp-controller="Home" asp-action="Index">Volunteer</a>
            <a asp-area="" asp-controller="Home" asp-action="Index">Locations</a>

        </div>
    </footer>
</body>
</html>

